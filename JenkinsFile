
pipeline {
    agent any
    stages {
        stage('Checkout project') {
            steps {
                script {
                    git branch: "master",
                        credentialsId: 'github_iyede_wsl_key',
                        url: 'git@github.com:iyedexe/reactor.git'
                }
            }
        }

        stage('Installing dependencies') {
            steps {
                script {
                    sh 'pip install -r requirements.txt'
                }
            }
        }
    }

}